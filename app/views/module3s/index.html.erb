<h1>Listing module3s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @module3s.each do |module3| %>
      <tr>
        <td><%= module3.name %></td>
        <td><%= link_to 'Show', module3 %></td>
        <td><%= link_to 'Edit', edit_module3_path(module3) %></td>
        <td><%= link_to 'Destroy', module3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Module3', new_module3_path %>
