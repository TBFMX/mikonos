<h1>Listing module4s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @module4s.each do |module4| %>
      <tr>
        <td><%= module4.name %></td>
        <td><%= link_to 'Show', module4 %></td>
        <td><%= link_to 'Edit', edit_module4_path(module4) %></td>
        <td><%= link_to 'Destroy', module4, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Module4', new_module4_path %>
